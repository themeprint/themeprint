{
  "name": "@themeprint/storybook-addon",
  "version": "0.1.1",
  "description": "themeprint Storybook addon.",
  "source": "src/index.js",
  "main": "dist/storybook-addon.js",
  "module": "dist/storybook-addon.esm.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/themeprint/themeprint.git",
    "directory": "packages/storybook-addon"
  },
  "homepage": "https://github.com/themeprint/themeprint/tree/master/packages/storybook-addon#readme",
  "scripts": {
    "clean": "yarn cache clean",
    "prepare": "microbundle --no-compress -o dist",
    "watch": "microbundle watch --no-compress -o dist",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@themeprint/babel-preset-themeprint": "^0.1.1-alpha.0",
    "@themeprint/jsx": "^0.1.1-alpha.0",
    "@themeprint/part": "^0.1.1-alpha.0",
    "@storybook/addons": "^5.3.12",
    "@storybook/api": "^5.3.12",
    "@storybook/client-logger": "^5.3.12",
    "@storybook/components": "^5.3.12",
    "@storybook/core-events": "^5.3.12",
    "@storybook/theming": "^5.3.12",
    "@utilz/types": "^0.1.11"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "jest": "^25.1.0",
    "microbundle": "^0.11.0",
    "prettier": "^1.19.1"
  },
  "peerDependencies": {
    "react": "*"
  }
}
